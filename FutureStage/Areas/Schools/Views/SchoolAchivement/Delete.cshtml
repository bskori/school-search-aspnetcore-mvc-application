@model SchoolAchivement

@{ 
    ViewData["Title"] = "Remove School Achivement";
}

<div class="row">
    <div class="col-md-6 offset-md-3">
        <h3 class="alert alert-danger text-center my-5">Are you sure, you want to delete this?</h3>
        <form asp-action="Delete">
            <a class="btn btn-outline-secondary" asp-action="Index">Show All</a>
            <input type="submit" class="btn btn-danger text-white float-end" value="Confirm" />
        </form>
    </div>
</div>