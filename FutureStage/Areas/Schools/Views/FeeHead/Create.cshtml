@model FeeHead

@{ 
    ViewData["Title"] = "Add new FeeHead";
}



<div class="p-2 col-md-8 offset-md-2">
    <h3 class="alert alert-primary text-center p-0 mb-5">Add new Fee Head</h3>

    <form asp-action="Create">
        <div class="mb-3">
            <label  asp-for="FeeHeadTitle" class="form-label"> </label>
            <input class="form-control" asp-for="FeeHeadTitle">
            <span class="text-danger" asp-validation-for="FeeHeadTitle"></span>
        </div>
        <div class="mb-3">
            <label  class="form-label" asp-for="FeeHeadDescription"></label>
            <textarea asp-for="FeeHeadDescription" class="form-control"></textarea>
        </div>
        <a asp-action="Index" class="btn btn-outline-secondary" >Show All</a>
        <input type="submit" value="Create" class="btn btn-outline-success float-end" />
    </form>
</div>
    
  